{
  "source": ["properties/**/*.json", "properties/**/*.js"],
  "platforms": {
    "js": {
      "transformGroup": "js",
      "transforms": ["name/cti/camel"],
      "buildPath": "dist/js/",
      "files": [
        {
          "format": "javascript/es6",
          "destination": "variables.js"
        }
      ]
    }
  }
}
